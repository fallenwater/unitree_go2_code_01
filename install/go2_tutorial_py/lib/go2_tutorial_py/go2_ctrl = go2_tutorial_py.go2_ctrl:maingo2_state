#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'go2-tutorial-py==0.0.0','console_scripts','go2_ctrl = go2_tutorial_py.go2_ctrl:maingo2_state'
__requires__ = 'go2-tutorial-py==0.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('go2-tutorial-py==0.0.0', 'console_scripts', 'go2_ctrl = go2_tutorial_py.go2_ctrl:maingo2_state')()
    )
